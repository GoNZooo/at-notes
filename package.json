{
  "name": "at-notes",
  "displayName": "at-notes",
  "description": "Highlighter and code search for at-notes",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "grammars": [
      {
        "path": "./syntaxes/at-notes.tmGrammar.json",
        "scopeName": "at-note.injection",
        "injectTo": [
          "source",
          "text"
        ],
        "tokenTypes": {
          "meta.embedded.at-note.todo": "other",
          "meta.embedded.at-note.note": "other",
          "meta.embedded.at-note.warn": "other",
          "meta.embedded.at-note.research": "other"
        }
      }
    ],
    "problemMatchers": [
      {
        "name": "at-note-todo",
        "fileLocation": ["relative", "${workspaceFolder}"],
        "pattern": {
          "regexp": "^(.+):(\\d+):(\\d):(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        },
        "severity": "warning"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.38.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.2.0"
  },
  "repository": {
    "url": "https://github.com/GoNZooo/at-notes"
  },
  "publisher": "GoNZooo",
  "license": "BSD-2-Clause"
}
